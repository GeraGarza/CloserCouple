<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.CloserCouple" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/active_icon</item>
        <item name="colorPrimaryVariant">@color/active_icon</item>
        <item name="colorOnPrimary">@color/active_icon</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/active_icon</item>
        <item name="colorSecondaryVariant">@color/active_icon</item>
        <item name="colorOnSecondary">@color/active_icon</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.CloserCouple.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>



</resources>